{
  "name": "gcworld/container",
  "description": "GCWorld Container",
  "authors": [
    {
      "name": "GameCharmer",
      "email": "gcworldind@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "GCWorld\\Container\\": "src/"
    }
  },
  "require": {
    "php":">=8.1",
    "ext-json": "*",
    "gcworld/common":"^2.7.2",
    "gcworld/database": "^2.6.2",
    "gcworld/globals": "^3.3",
    "gcworld/interfaces": "^4.2.0",
    "gcworld/objectmanager": "^2.8.0",
    "gcworld/orm": "^6.3.3",
    "gcworld/routing": "^5.1.0",
    "gcworld/utilities": "^2.0.0",
    "symfony/yaml": "^6.2.7",
    "ramsey/uuid": "^4.6",
    "psr/container": "^2.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.9.3",
    "phpstan/phpstan-deprecation-rules": "^1.0.0",
    "friendsofphp/php-cs-fixer": "^3.14",
    "gcworld/code_sniffer_contrib": "^2.1.1",
    "squizlabs/php_codesniffer": "^3.7.1",
    "phpmd/phpmd": ">=2.13",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "povils/phpmnd": "^3.0",
    "roave/security-advisories": "dev-latest"
  },
  "license":"proprietary",
  "config": {
    "discard-changes": true,
    "secure-http" : false,
    "allow-plugins": {
      "composer/installers": true,
      "php-http/discovery": true
    }
  }
}
